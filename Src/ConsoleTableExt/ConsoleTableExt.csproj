<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net45;net35</TargetFrameworks>
        <Title>ConsoleTableExt</Title>
        <Version>2.0.3</Version>
        <Copyright>Copyright 2017 minhhungit</Copyright>
        <Description>A fluent library to print out a nicely formatted table in a console application C#</Description>
        <Authors>Hung Vo</Authors>
        <PackageLicenseUrl>https://github.com/minhhungit/ConsoleTableExt/blob/master/LICENSE</PackageLicenseUrl>
        <PackageProjectUrl>https://github.com/minhhungit/ConsoleTableExt/</PackageProjectUrl>
        <RepositoryUrl>https://github.com/minhhungit/ConsoleTableExt/</RepositoryUrl>
        <PackageTags>console text-table fluent console-table</PackageTags>
        <PackageReleaseNotes>support .net framework 3.5 (thanks maikebing)</PackageReleaseNotes>
        <AssemblyVersion>2.0.3</AssemblyVersion>
        <FileVersion>2.0.3</FileVersion>
        <Company />
        <PackageIconUrl>https://raw.githubusercontent.com/minhhungit/ConsoleTableExt/master/wiki/Images/icon.ico</PackageIconUrl>
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>strongname.snk</AssemblyOriginatorKeyFile>
        <DelaySign>false</DelaySign>
    </PropertyGroup>
</Project>